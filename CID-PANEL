    Title = "Strongest Battlegrounds",
    Icon = "solar:play-circle-bold", IconAlign = "Right",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/S-PScripts/S-P-Hub/main/Main.lua"))()
    end
})

-- --- SETTINGS ---
local SettingsTab = Window:Tab({ Title = "Settings", Icon = "solar:settings-minimalistic-bold", IconColor = Color3.fromRGB(200,200,200) })

local Cheats = SettingsTab:Section({ Title = "Player Utility" })
Cheats:Slider({ Title = "Walkspeed", Value = { Min = 16, Max = 250, Default = 16 }, Callback = function(v) pcall(function() game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v end) end })
Cheats:Slider({ Title = "Jump Power", Value = { Min = 50, Max = 500, Default = 50 }, Callback = function(v) pcall(function() game.Players.LocalPlayer.Character.Humanoid.JumpPower = v end) end })

local System = SettingsTab:Section({ Title = "System" })
System:Toggle({ 
    Title = "Anti-AFK", 
    Callback = function(v)
        _G.AntiAFK = v
        game.Players.LocalPlayer.Idled:Connect(function()
            if _G.AntiAFK then game:GetService("VirtualUser"):Button2(Vector2.new(), workspace.CurrentCamera.CFrame) end
        end)
    end
})

System:Button({
    Title = "Rejoin",
    Icon = "solar:restart-bold",
    Callback = function() game:GetService("TeleportService"):Teleport(game.PlaceId) end
})

-- --- CREDITS ---
local Info = Window:Tab({ Title = "Credits", Icon = "solar:info-circle-bold", IconColor = Green })
Info:Section({ Title = "THE CID PANEL v2.5", FontWeight = Enum.FontWeight.Bold })
Info:Button({ 
    Title = "Copy Discord Link", 
    Icon = "solar:copy-bold", 
    Callback = function() 
        setclipboard("https://discord.gg/2vxSdFF375") 
        WindUI:Notify({Title = "Copied", Content = "Discord invite copied!"})
    end 
})
