local WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/Footagesus/WindUI/main/dist/main.lua"))()

local Window = WindUI:CreateWindow({
    Title = "THE CID PANEL",
    Folder = "cid_panel",
    Icon = "solar:folder-2-bold-duotone",
    NewElements = true,
    OpenButton = {
        Title = "Open THE CID PANEL",
        Enabled = true,
        Draggable = true,
        Color = ColorSequence.new(Color3.fromHex("#FF4B4B"), Color3.fromHex("#7775F2"))
    }
})

-- Colors
local Red = Color3.fromHex("#FF4B4B")
local Green = Color3.fromHex("#10C550")
local Purple = Color3.fromHex("#7775F2")
local Yellow = Color3.fromHex("#ECA201")

local ElementsSection = Window:Section({ Title = "Game Scripts" })

-- Helper Function
local function AddGame(Tab, GameName, CodeSource)
    Tab:Button({
        Title = GameName,
        Icon = "solar:play-bold",
        IconAlign = "Right",
        Callback = function()
            WindUI:Notify({ Title = "THE CID PANEL", Content = "Booting " .. GameName, Duration = 3 })
            loadstring(game:HttpGet(CodeSource))()
        end
    })
    Tab:Space()
end

-- TABS
local GamesAE = ElementsSection:Tab({ Title = "Games A-E", Icon = "solar:star-bold", IconColor = Red, IconShape = "Square", Border = true })
local GamesFM = ElementsSection:Tab({ Title = "Games F-M", Icon = "solar:bolt-bold", IconColor = Green, IconShape = "Square", Border = true })
local GamesNZ = ElementsSection:Tab({ Title = "Games N-Z", Icon = "solar:target-bold", IconColor = Purple, IconShape = "Square", Border = true })

-- --- Games A-E (ADOPT ME AT TOP) ---
AddGame(GamesAE, "Adopt Me (AutoFarm)", "https://pastefy.app/71SURyDf/raw")
AddGame(GamesAE, "Adopt Me (Pet Spawner)", "https://pastefy.app/nTv8VJUz/raw")
AddGame(GamesAE, "Blox Fruits (Thunder Hub)", "https://raw.githubusercontent.com/TheRealGamer-hub/Thunder-Hub/main/Main.lua")
AddGame(GamesAE, "Arsenal", "https://raw.githubusercontent.com/0x0d3f/Quartyz/main/Gold.lua")
AddGame(GamesAE, "Brookhaven", "https://raw.githubusercontent.com/Ice-Hub/Ice-Hub/main/Ice-Hub.lua")
AddGame(GamesAE, "Bee Swarm Simulator", "https://raw.githubusercontent.com/Project-Advanced/CocoHub/main/BeeSwarmSimulator.lua")

-- --- Games F-M ---
AddGame(GamesFM, "Murder Mystery 2", "https://pastefy.app/OMoSSnvZ/raw")
AddGame(GamesFM, "Flee the Facility", "https://raw.githubusercontent.com/RTrade/Voidz/main/Games/FleeTheFacility.lua")
AddGame(GamesFM, "Frontlines", "https://raw.githubusercontent.com/SillyVane/Exunys-Scripts/main/Frontlines/Main.lua")
AddGame(GamesFM, "Jailbreak", "https://raw.githubusercontent.com/RegularVynixu/Vynixius/main/Jailbreak/Jailbreak.lua")
AddGame(GamesFM, "King Legacy", "https://raw.githubusercontent.com/Dragon-v2/D-v2/main/KingLegacy.lua")

-- --- Games N-Z ---
AddGame(GamesNZ, "Pet Simulator 99", "https://raw.githubusercontent.com/Zap-Hub/ZapHub/main/ZapHubLoader.lua")
AddGame(GamesNZ, "Rivals", "https://raw.githubusercontent.com/Sheeshablee73/Scripts/main/Rivals.lua")
AddGame(GamesNZ, "Sol's RNG", "https://raw.githubusercontent.com/NatsumeScripts/SolsRNG/main/Main.lua")
AddGame(GamesNZ, "Strongest Battlegrounds", "https://raw.githubusercontent.com/S-PScripts/S-P-Hub/main/Main.lua")
AddGame(GamesNZ, "Tower Defense Sim", "https://raw.githubusercontent.com/Xenon-Scripts/Xenon/main/Loader.lua")

-- --- SETTINGS & UTILS ---
local SettingsTab = Window:Tab({ Title = "Settings", Icon = "solar:settings-bold", IconColor = Color3.fromRGB(200,200,200), IconShape = "Square", Border = true })

local PlayerCheats = SettingsTab:Section({ Title = "Player Settings" })
PlayerCheats:Slider({ Title = "Walkspeed", Value = { Min = 16, Max = 250, Default = 16 }, Callback = function(v) game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v end })
PlayerCheats:Slider({ Title = "Jump Power", Value = { Min = 50, Max = 500, Default = 50 }, Callback = function(v) game.Players.LocalPlayer.Character.Humanoid.JumpPower = v end })

local Utils = SettingsTab:Section({ Title = "Utilities" })
Utils:Toggle({ Title = "Anti-AFK", Callback = function(v)
    _G.AntiAFK = v
    game:GetService("Players").LocalPlayer.Idled:Connect(function()
        if _G.AntiAFK then game:GetService("VirtualUser"):Button2(Vector2.new(), workspace.CurrentCamera.CFrame) end
    end)
end})

-- Credits
SettingsTab:Space()
local Credits = SettingsTab:Section({ Title = "Credits" })
Credits:Section({ Title = "This script was made by CID", FontWeight = Enum.FontWeight.Bold })

-- Info
local Info = Window:Tab({ Title = "Info & Socials", Icon = "solar:info-circle-bold", IconColor = Yellow, IconShape = "Square", Border = true })
Info:Button({ Title = "Copy Discord Invite", Desc = "discord.gg/2vxSdFF375", Icon = "solar:share-circle-bold", IconAlign = "Right", Callback = function() setclipboard("https://discord.gg/2vxSdFF375") end })
