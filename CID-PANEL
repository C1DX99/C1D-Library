-- [[ THE CID PANEL - PREMIUM & FAST ]] --

local WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/Footagesus/WindUI/main/dist/main.lua"))()

local Window = WindUI:CreateWindow({
    Title = "THE CID PANEL [PREMIUM]",
    Folder = "cid_panel_pro",
    Icon = "solar:crown-bold",
    NewElements = true,
    OpenButton = {
        Title = "CID MENU",
        Enabled = true,
        Draggable = true,
        Color = ColorSequence.new(Color3.fromHex("#FFD700"), Color3.fromHex("#FF4B4B"))
    }
})

-- COLORS
local Gold = Color3.fromHex("#FFD700")
local Red = Color3.fromHex("#FF4B4B")
local Blue = Color3.fromHex("#00BFFF")
local Green = Color3.fromHex("#10C550")

-- --- DASHBOARD (USER & SYSTEM INFO) ---
local Dashboard = Window:Tab({ Title = "Dashboard", Icon = "solar:home-smile-bold", IconColor = Gold })

local UserInfo = Dashboard:Section({ Title = "User Information" })
UserInfo:Section({ Title = "User: " .. game.Players.LocalPlayer.Name, TextTransparency = 0.5 })
UserInfo:Section({ Title = "Device: " .. (game:GetService("UserInputService").TouchEnabled and "Mobile" or "PC"), TextTransparency = 0.5 })

local Stats = Dashboard:Section({ Title = "System Performance" })
local TimeLabel = Stats:Section({ Title = "Time: --:--:--" })
local PingLabel = Stats:Section({ Title = "Ping: -- ms" })

task.spawn(function()
    while task.wait(1) do
        TimeLabel:SetTitle("Time: " .. os.date("%X"))
        PingLabel:SetTitle("Ping: " .. math.floor(game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue()) .. " ms")
    end
end)

-- --- TABS ---
local MainGames = Window:Tab({ Title = "Main Games", Icon = "solar:star-bold", IconColor = Red })
local OtherGames = Window:Tab({ Title = "Other Games", Icon = "solar:gamepad-old-bold", IconColor = Blue })

-- [[ MAIN GAMES - YOUR CHOSEN LINKS ]]
MainGames:Button({
    Title = "Adopt Me (Gingerbread)",
    Icon = "solar:play-circle-bold", IconAlign = "Right",
    Callback = function()
        loadstring(game:HttpGet('https://pastefy.app/71SURyDf/raw'))()
    end
})
MainGames:Space()

MainGames:Button({
    Title = "Adopt Me (Pet Spawner)",
    Icon = "solar:play-circle-bold", IconAlign = "Right",
    Callback = function()
        loadstring(game:HttpGet('https://pastefy.app/nTv8VJUz/raw'))()
    end
})
MainGames:Space()

MainGames:Button({
    Title = "Murder Mystery 2",
    Icon = "solar:play-circle-bold", IconAlign = "Right",
    Callback = function()
        loadstring(game:HttpGet('https://pastefy.app/OMoSSnvZ/raw'))()
    end
})
MainGames:Space()

MainGames:Button({
    Title = "Brookhaven (Premium)",
    Icon = "solar:play-circle-bold", IconAlign = "Right",
    Callback = function()
        loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/fda9babd071d6b536a745774b6bc681c.lua"))()
    end
})

-- [[ OTHER GAMES - VERIFIED 2025 ]]
OtherGames:Button({
    Title = "Blox Fruits (Premium)",
    Icon = "solar:play-circle-bold", IconAlign = "Right",
    Callback = function()
        loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/fda9babd071d6b536a745774b6bc681c.lua"))()
    end
})
OtherGames:Space()

OtherGames:Button({
    Title = "Arsenal (Verified)",
    Icon = "solar:play-circle-bold", IconAlign = "Right",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/0x0d3f/Quartyz/main/Gold.lua"))()
